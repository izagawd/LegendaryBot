@page "/userdata"
@using System.Security.Claims
@using Website.Components.Navigation

@inject AuthenticationStateProvider Prov
@code
{
    ClaimsPrincipal _user = null!;
    protected override async Task OnInitializedAsync()
    {
        _user = (await Prov.GetAuthenticationStateAsync()).User;
    }
}
<Navigation>
 @_user.Claims.Count()
</Navigation>