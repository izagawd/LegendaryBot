@page "/userdata"
@using System.Security.Claims
@using Website.Components.Navigation

@inject AuthenticationStateProvider prov
@code
{
    ClaimsPrincipal user;
    protected override async Task OnInitializedAsync()
    {
        user = (await prov.GetAuthenticationStateAsync()).User;
    }
}
<Navigation>
 @user.Claims.Count()
</Navigation>